                        userId="root"
                        password="199415">
        </jdbcConnection>

        <!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer，为 true时把JDBC DECIMAL 和
           NUMERIC 类型解析为java.math.BigDecimal -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>


        <!--targetPackage 目录包名
            targetProject 目录路径 最好是绝对路径
            -->
        <!-- dto class -->
        <javaModelGenerator targetPackage="com.jsh.erp.datasource.entities"
                            targetProject="/Users/zuochong/Documents/IDEAWorkSpace/XiaoLeErp/src/main/java">
            <property name="enableSubPackages" value="false"/>
            <property name="trimStrings" value="true"/>
            <!--继承父类-->
            <property name="rootClass" value="com.jsh.erp.datasource.BaseEntity"/>
        </javaModelGenerator>

        <!-- mybatis xml file -->
        <sqlMapGenerator targetPackage="com.jsh.erp.datasource.dtos"
                         targetProject="/Users/zuochong/Documents/IDEAWorkSpace/XiaoLeErp/src/main/java">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>

        <!-- mapper class -->
